{
    "version": 2,
    "builds": [
        {
            "src": "./src/server/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "./src/client/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
}